<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.PhoenixHospital.doctor_attendance.dao.IAttendanceDao">
    <select id="getAttendanceList" resultType="com.PhoenixHospital.doctor_attendance.vo.AttendanceVO">
        SELECT
            attendance_id,
            dc_id,
            treatment_code,
            attendance_date,
            attendance_time,
            reservation_yn
        FROM doctor_attendance
        WHERE dc_id=#{dcId}
        AND reservation_yn='N'
    </select>
</mapper>